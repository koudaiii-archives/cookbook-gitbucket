{
  "name": "base",
  "chef_type" : "role",
  "json_class" : "Chef::Role",
  "override_attributes": {
    "my_jetty" : {
      "host" : "127.0.0.1"
    },
    "java" : {
      "install_flavor" : "oracle",
      "jdk_version" : "7",
      "oracle" : {
        "accept_oracle_download_terms" : true
      }
    }
  },
  "run_list": [
    "recipe[my_timezone]",
    "recipe[my_ntp]",
    "recipe[java]",
    "recipe[my_jetty]"
  ]
}
